class Solution
{
    public:
    void deleteMid(stack<int>&s, int n)
    {
        int m=0;
        m=n/2;
        m=m+1;
        int j=1;
        stack<int>st;
        while(!s.empty()){
            if(j!=m){
                st.push(s.top());
            }
            j++;
            s.pop();
        }
        while(!st.empty()){
            s.push(st.top());
            st.pop();
        }
    }
};
