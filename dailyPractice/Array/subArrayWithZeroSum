bool subArrayExists(int arr[], int n)
{
    //Your code here
    unordered_map<int,int>m;
    int s=0;
    for(int i=0; i<n; i++){
        s+=arr[i];
        if(arr[i]==0||s==0)return true;
        if(m.find(s)!=m.end()){
            return true;
        }
        m.insert({s,1});
    }
    return false;
}
