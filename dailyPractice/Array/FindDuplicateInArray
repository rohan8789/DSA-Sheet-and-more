//Find duplicate in array containing elements from 0-n. do in constant space

class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        for(int i=0; i<nums.size();){
            if(nums[i]!=nums[nums[i]]){
                swap(nums[i], nums[nums[i]]);
            }else{
                i++;
            }
        }
        for(int i=0; i<nums.size(); i++){
            if(nums[i]!=i){
                return nums[i];
            }
        }
        return -1;
    }
};
