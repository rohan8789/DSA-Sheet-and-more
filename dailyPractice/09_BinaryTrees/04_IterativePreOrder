Iterative implementation of ProOrder Traversal



class Solution{
    public:
    vector<int> preOrder(Node* root)
    {
        vector<int>v;
        stack<Node*>s;
        Node* curr=root;
        while(curr!=NULL||s.empty()==false){
            while(curr!=NULL){
                v.push_back(curr->data);
                s.push(curr);
                curr=curr->left;
            }
            curr=s.top();
            s.pop();
            curr=curr->right;
        }
        return v;
    }
};
